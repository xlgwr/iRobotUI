<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>xlgwr iRobotUI</title>
 <link href="http://xlgwr.github.com/iRobotUI/atom.xml" rel="self"/>
 <link href="http://xlgwr.github.com/iRobotUI/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://xielinggen.cn/</id>
 <author>
   <name>xlgwr iRobotUI</name>
   <email>xlgwr@163.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://xlgwr.github.com/iRobotUI{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://xlgwr.github.com/iRobotUI{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>